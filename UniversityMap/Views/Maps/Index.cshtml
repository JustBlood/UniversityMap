<div class="block-body">
@model Map
@section Scripts{
    @{
        if (Model != null)
        {
                <link rel="stylesheet" media="all" href="https://jvectormap.com/css/jquery-jvectormap-2.0.3.css"/>
                <script src="https://jvectormap.com/js/jquery-1.8.2.min.js"></script>
                <script src="https://jvectormap.com/js/jquery-jvectormap-2.0.3.min.js"></script>
                <script>@Html.Raw(Model.JQueryScript)</script>
                <script src="~/js/jvectormap-creater.js"></script>
                <script src="~/js/wraplink_main.js"></script>  
        }
    }
}
<h1>Выберите корпус для просмотра подробной карты:</h1>


@foreach (var building in (string[])ViewData["Names"])
{
    if (building != "CFU")
    {
        <button onclick="document.location='@ViewData["Url"]/@building'">Корпус @building</button>
    }
        
}

<div class="container" id="map" style="width: 100%; height: 600px"></div>
</div>