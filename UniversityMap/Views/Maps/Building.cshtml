@model Map
@section Scripts{
    @{
        if (Model != null)
        {
    <link rel="stylesheet" media="all" href="https://jvectormap.com/css/jquery-jvectormap-2.0.3.css"/>
    <script src="https://jvectormap.com/js/jquery-1.8.2.min.js"></script>
    <script src="https://jvectormap.com/js/jquery-jvectormap-2.0.3.min.js"></script>
    <script>@Html.Raw(Model.JQueryScript)</script>
    <script src="~/js/jvectormap-creater.js"></script>
    <script src="~/js/wraplink.js"></script>  
        }
    }
}

<h1>Корпус: @ViewData["Building"]</h1>
@{
    if (Model == null)
    {
    <h1>Выбранный этаж недоступен, пожалуйста, выберите другой...</h1>
    }
}

@foreach (var floor in (int[])ViewData["Floors"])
{
    <button onclick="document.location='@ViewData["Url"]?floor=@floor'">@floor</button>
}

<div class="container" id="map" style="width: 100%; height: 900px"></div>